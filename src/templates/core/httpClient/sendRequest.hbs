async function sendRequest(options: ApiRequestOptions, url: string): Promise<any> {
    let body;
    switch (options.method) {
        case 'GET':
            body = await GET(url);
            break;
        case 'POST':
            body = await POST(url, getRequestBody(options));
            break;
        case 'PUT':
            body = await PUT(url, getRequestBody(options));
            break;
        case 'DELETE':
            body = await DELETE(url, getRequestBody(options));
            break;
    }

    return {body};
}
